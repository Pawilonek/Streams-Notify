{
   "background": {
      "persistent": true,
      "scripts": [ "vendor/jquery/jquery.min.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "files/icon.png",
      "default_popup": "popup.html"
   },
   "description": "Rozszerzenie wyświetla aktualny status wybranych streamów oraz powiadamia o ich rozpoczęciu.",
   "homepage_url": "http://pawilonek.2ap.pl/",
   "icons": {
      "128": "files/icon128.png",
      "48": "files/icon48.png"
   },
   "manifest_version": 2,
   "name": "Streams Notify",
   "optional_permissions": [ "background" ],
   "permissions": [ "https://api.twitch.tv/kraken/*", "http://static-cdn.jtvnw.net/*", "http://api.hitbox.tv/*", "http://edge.sf.hitbox.tv/*", "http://api.gaminglive.tv/*", "notifications", "storage"],
   "version": "2.0.2"
}
